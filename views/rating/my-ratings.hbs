<div>
    <h2>My ratings</h2>
    {{#each myRating}}

    {{#each this.restaurant}}
    <h3> {{this.name}}</h3>
    {{/each}}
    <p>Rating: {{this.rating}}</p>
    <p>Commentary: {{this.commentary}}</p>
    
    <a href="/rating/{{this._id}}/my-ratings/edit"><button>Edit</button></a>
    <br>
    <form action="/rating/{{this._id}}/my-ratings/delete" method="POST"><button>Delete</button></form>
    
    {{/each}}

    <a href="/profile"><button>Go back</button></a>
</div>